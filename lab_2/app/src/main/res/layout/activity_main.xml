<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/closest_and_fethest_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fetherest and closest"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/all_data_button" />

    <Button
        android:id="@+id/profile_button"
        android:layout_width="235dp"
        android:layout_height="42dp"
        android:text="developer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button8" />

    <Button
        android:id="@+id/all_data_button"
        android:layout_width="230dp"
        android:layout_height="43dp"
        android:layout_marginTop="132dp"
        android:text="All data"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/big_cities_near_kyiv_button"
        android:layout_width="233dp"
        android:layout_height="51dp"
        android:text="Big cities near Kyiv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/closest_and_fethest_button" />

    <Button
        android:id="@+id/button8"
        android:layout_width="234dp"
        android:layout_height="48dp"
        android:text="Contacts"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/big_cities_near_kyiv_button" /><![CDATA[

























    app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.166" />









]]>
</androidx.constraintlayout.widget.ConstraintLayout>